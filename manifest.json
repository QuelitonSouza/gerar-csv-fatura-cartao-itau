{
  "manifest_version": 3,
  "name": "Itau Gerar CSV",
  "short_name": "Itau CSV",
  "description": "Adiciona botão para exportação da fatura em aberto para CSV",
  "version": "2.0.0",
  "icons": {
    "16": "itau-16.png",
    "32": "itau-16.png",
    "48": "itau-48.png",
    "128": "itau-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.itau.com.br/*"],
      "exclude_matches": [
      ],
      "js": ["extrato-itau.js"],
      "run_at": "document_idle",
      "all_frames": false
    }
  ]
}
