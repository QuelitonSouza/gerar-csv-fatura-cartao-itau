{
  "manifest_version": 3,
  "name": "Itaú Exportar Fatura CSV/OFX",
  "short_name": "Itaú Fatura",
  "description": "Exporte sua fatura do cartão Itaú para CSV ou OFX. Compatível com Excel, Google Sheets e apps financeiros.",
  "version": "3.0.7",
  "icons": {
    "16": "itau-16.png",
    "32": "itau-16.png",
    "48": "itau-48.png",
    "128": "itau-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.itau.com.br/*",
        "https://internetpf6.itau.com.br/*",
        "https://internetpf5.itau.com.br/*",
        "https://internetpf4.itau.com.br/*"
      ],
      "exclude_matches": [],
      "js": [
        "extrato-itau.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ]
}